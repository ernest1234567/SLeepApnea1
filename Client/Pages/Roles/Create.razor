@page "/roless/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New roless</h3>
<hr />
<br />

<FormComponent ButtonText="Create roles" roless="roles" OnValidSubmit="@Createroles" />

@code {
	Roles roles = new Roles();

	private async Task Createroles()
	{
		await _client.PostAsJsonAsync(Endpoints.RolessEndpoint, roles);
		_navManager.NavigateTo("/roless/");
	}
}
